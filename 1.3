#include <math.h>
#include <stdio.h>
#include <stdlib.h>

int get_power(int number, int power)
{
    if (number==0)
        return 1;
    else if (number==1)
        return power;
    else
        return pow( number * number, power /2)*number;
}
int main()
{
    int number=2, power=6;
    //scanf("%d%d", &number, &power);
    printf("%d", get_power(number, power));
    return 0;
}
